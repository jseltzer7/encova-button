{
  "name": "encova-button-stencil-component",
  "version": "0.0.8",
  "description": "Component Library being developed for testing using StencilJS",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/encova-button-stencil-component/encova-button-stencil-component.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "build.watch": "stencil build --watch",
    "build-sb": "build-storybook -c .storybook -o build"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@stencil/core": "^1.12.2",
    "@storybook/html": "^5.3.19",
    "babel-loader": "^8.1.0"
  },
  "license": "MIT",
  "dependencies": {
    "@storybook/addon-knobs": "^5.3.19",
    "typescript": "^3.8.3"
  },
  "author": "Josh Seltzer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jseltzer7/encova-button.git"
  },
  "bugs": {
    "url": "https://github.com/jseltzer7/encova-button/issues"
  },
  "homepage": "https://github.com/jseltzer7/encova-button#readme"
}
